		<script>
			jQuery('#done_reg').on('click', function(e) {
				e.preventDefault();
				var  send = true;
				jQuery('.eq_l').each(function(){
    if(!jQuery(this).val() || jQuery(this).val() == ''){
       jQuery(this).css('border-color','red');//Сделаем бордер красным
       send = false;
    }
});
				if(!send) return false;
				
				jQuery('.result_save_ajax_profile').html('');
				let name = jQuery('#name_login').val();
				let pass = jQuery('#pass_login').val();
				jQuery.ajax({
			url: "/wp-admin/admin-ajax.php",
			method: 'POST',
			data: {
                action: 'login_author_new',
                name:name,
                pass:pass,
               
				
				
			},
			success: function(data) {
               
              if(data == '2') {
				  window.location = '/account/?uid=new';
				   jQuery('.result_save_ajax_profile').append('Успешная авторизация! Открываем кабинет....');
			  } else {
				  jQuery('.result_save_ajax_profile').append(data);
			  }
			  if(data.includes('<a')) {
			  
		jQuery('.result_save_ajax_profile a').attr('href', '/forgot/');


			  }
                
               
                
               },
			Error: function(data) {
                
			}
		});
				
			});
			
			
			
			jQuery('#reg_new_user').on('click', function(o) {
				o.preventDefault();
				var spesial = jQuery('#special').prop('checked');
				
				var  send = true;
				jQuery('.eq').each(function(){
    if(!jQuery(this).val() || jQuery(this).val() == ''){
       jQuery(this).css('border-color','red');//Сделаем бордер красным
       send = false;
    }
});
				if(!send) return false;
      
        jQuery('.result_save_ajax_profile').html('');
				let first_name = jQuery('#first_name').val();
				let last_name = jQuery('#last_name').val();
        let pass_reg = jQuery('#pass_reg').val();
        let pass2_reg = jQuery('#pass2_reg').val();
        let name_reg = jQuery('#name_reg').val();
        let email_reg = jQuery('#email_reg').val();
		let wall = jQuery('#wall').val();
      
        
        if (pass_reg == pass2_reg) {
            if(jQuery("#rules").is(':checked')) {
				
            jQuery('#reg_new_user').prop('disabled', true);
        
    jQuery.ajax({
			url: "/wp-admin/admin-ajax.php",
			method: 'POST',
			data: {
                action: 'register_new_user_custom_simple',
                name:name_reg,
				first_name:first_name,
				last_name:last_name,
                pass:pass_reg,
                email:email_reg,
				wall:wall,
				special:spesial
				
				
			},
			success: function(data) {
               
              if(data == '1') {
				  window.location = '/account/';
				   jQuery('.result_save_ajax_profile').append('Вы зарегистрированы! Открываем кабинет...');
			  } else {
				  jQuery('.result_save_ajax_profile').append(data);
				  jQuery('#reg_new_user').prop('disabled', false);
			  }
                
                
                
               },
			Error: function(data) {
                
			}
		}); } else {
        jQuery('.result_save_ajax_profile').append('<p>Нужно согласиться с правилами</p>');
        
    } } else {
        
        jQuery('.result_save_ajax_profile').append('<p>Пароли не совпадают</p>');
        
    } 
        
        });
		 
		
		</script>
